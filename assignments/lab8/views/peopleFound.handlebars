
<h1>People Found</h1>
<h2>{{searchPersonName}}</h2>
<main>
    
    <ul>
        {{#each resultPeople}}
        <li>
            <a href="/persondetails/{{this.id}}">{{this.firstName}} {{this.lastName}}</a>
        </li>
        {{/each}}
        
    </ul>

    <a class= "another-search" href="http://localhost:3000/">Make another search</a>
</main>